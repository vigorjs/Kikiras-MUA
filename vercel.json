{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/*/*/*/(.*)",
            "dest": "/public/*/*/(.*)"
        },
        {
            "src": "/*/*/(.*)",
            "dest": "/public/*/(.*)"
        },
        {
            "src": "/*/(.*)",
            "dest": "/public/(.*)"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://makeupbykikiraa.vercel.app",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie",
        "FILESYSTEM_DRIVER": "public"
    },
    "outputDirectory": "public"
}
